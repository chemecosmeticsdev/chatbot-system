{
  "name": "chatbot-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:api": "node scripts/test-apis.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:auth": "playwright test tests/e2e/authentication.spec.ts",
    "test:api:e2e": "playwright test tests/e2e/api-integration.spec.ts",
    "test:core": "playwright test tests/e2e/core-functionality.spec.ts",
    "test:all": "npm run lint && npm run type-check && npm run build && npm run test:api && npm run test:e2e",
    "deploy:validate": "gh workflow run deploy-validation.yml",
    "ci:local": "npm run lint && npm run type-check && npm run build && npm run test:api",
    "format": "prettier --write .",
    "setup-env": "node scripts/setup-env.js",
    "postinstall": "playwright install-deps || echo 'Playwright setup will be completed later'",
    "i18n:init": "node scripts/i18n-manager.js init",
    "i18n:validate": "node scripts/i18n-manager.js validate",
    "i18n:extract": "node scripts/i18n-manager.js extract",
    "i18n:report": "node scripts/i18n-manager.js report",
    "i18n:backup": "node scripts/i18n-manager.js backup",
    "i18n:check": "npm run i18n:validate -- --detailed --strict"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.478.0",
    "@aws-sdk/client-s3": "^3.478.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^10.12.0",
    "@stackframe/stack": "^2.5.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^15.0.0",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0"
  }
}
